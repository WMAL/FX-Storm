    If d = 29 And n = 2 And (g / 4) <> Int(g / 4) Then GoTo prestupna
    p = g
    m = n - 2
    If n > 2 Then GoTo ef
    p = p - 1
    m = m + 2
ef:
    C = Int(p / 100)
    Y = p - C * 100
    w = d + Int((13 * m - 1) / 5) + Y + Int(Y / 4) + Int(C / 4) - 2 * C
    k = w - 7 * Int(w / 7)
    If k = 0 Or k + 1 = 7 Then thedate = "Sun"
    If k = 1 Then thedate = "Mon"
    If k = 2 Then thedate = "Tue"
    If k = 3 Then thedate = "Wed"
    If k = 4 Then thedate = "Thu"
    If k = 5 Then thedate = "Fri"
    If k = 6 Then thedate = "Sat"
    Exit Sub
er:
   
    Exit Sub
prestupna:
  
    Exit Sub
errrrrrr:
    'MsgBox "Error", vbExclamation, "Error"
End Sub


