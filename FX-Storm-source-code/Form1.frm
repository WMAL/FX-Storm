    arraycount = 33
    
    'get current month
    Set Litem = ListView1.ListItems.Item(Form1.ListView1.ListItems.Count)
    thedatevalue = Litem.ListSubItems.Item(1)
    thesplit2 = Split(thedatevalue, "\")
    themonth = thesplit2(1)
    
    'find month keyword
    If themonth > 1 Then
        targetmonth = themonth - 1
    Else
        targetmonth = 12
    End If
    
    'get all  rows of target month
    xcounter2 = 1
    For xcounter = 1 To Form1.ListView1.ListItems.Count
            Set Litem = ListView1.ListItems.Item(xcounter)
            thedatevalue = Litem.ListSubItems.Item(1)
            thesplit2 = Split(thedatevalue, "\")
            themonth = thesplit2(1)
            If themonth = targetmonth Then
                pairlistvalues(xcounter2) = thedatevalue & vbTab & Litem.ListSubItems.Item(3) & vbTab & Litem.ListSubItems.Item(4) & vbTab & Litem.ListSubItems.Item(5)
                xcounter2 = xcounter2 + 1
            End If
    Next xcounter
    
    'get close value
    Dim i As Integer
    For i = 1 To arraycount
        If pairlistvalues(i) <> "" Then
            thesplit2 = Split(pairlistvalues(i), vbTab)
            Text4.Text = thesplit2(3)
        End If
    Next i
    
    'get highst value
    For i = 1 To arraycount
        If pairlistvalues(i) <> "" Then
            thesplit2 = Split(pairlistvalues(i), vbTab)
            thehighvalue(i) = thesplit2(1)
        End If
    Next i
    Call Hight_Score(thehighvalue(), arraycount)
    
    
    'get lowest value
    For i = 1 To arraycount
        If pairlistvalues(i) <> "" Then
            thesplit2 = Split(pairlistvalues(i), vbTab)
            thelowvalue(i) = thesplit2(2)
            'MsgBox thelowvalue(i)
        End If
    Next i
    Call low_Score(thelowvalue(), arraycount)
    
    
End Sub


'get highest
Public Sub Hight_Score(Number() As Double, counter As Integer)
    Dim Index As Integer
    Dim h_score As Double
    h_score = Number(1)
    For Index = 1 To counter
        If Number(Index) > h_score Then
            h_score = Number(Index)
        End If
    Next Index
    Text3.Text = h_score

End Sub


'get highest
Public Sub low_Score(Number() As Double, counter As Integer)
    Dim Index As Integer
    Dim h_score As Double
    h_score = Number(1)
    For Index = 1 To counter
        If Number(Index) < h_score And Number(Index) <> 0 Then
            h_score = Number(Index)
        End If
    Next Index
    Text2.Text = h_score

End Sub


