Public Sub getfibona(thetext As TextBox, thevalue As Double, theplus As Boolean)

    Dim tlow As Double, thigh As Double
    Dim thefirst As Double
    Dim theseco As Double
   
    thigh = Text3.Text
    tlow = Text2.Text
    
    
    
    If theplus = True Then
        thefirst = (thigh - tlow) * thevalue
        theseco = thefirst + tlow
    Else
        thefirst = (thigh - tlow) * thevalue
        theseco = thigh - thefirst
    End If
    thetext.Text = Round(theseco, 4)
End Sub


